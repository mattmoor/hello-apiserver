load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "go_default_library",
    srcs = [
        "collelem.go",
        "colltab.go",
        "contract.go",
        "export.go",
        "numeric.go",
        "table.go",
        "trie.go",
    ],
    importpath = "golang.org/x/text/collate/colltab",
    visibility = ["//visibility:public"],
    deps = ["//vendor/golang.org/x/text/unicode/norm:go_default_library"],
)

go_test(
    name = "go_default_test",
    srcs = [
        "collelem_test.go",
        "colltab_test.go",
        "contract_test.go",
        "numeric_test.go",
        "trie_test.go",
    ],
    importpath = "golang.org/x/text/collate/colltab",
    library = ":go_default_library",
    deps = ["//vendor/golang.org/x/text/internal/testtext:go_default_library"],
)

go_test(
    name = "go_default_xtest",
    srcs = ["collate_test.go"],
    importpath = "golang.org/x/text/collate/colltab_test",
    deps = [
        "//vendor/golang.org/x/text/collate:go_default_library",
        "//vendor/golang.org/x/text/language:go_default_library",
        "//vendor/golang.org/x/text/unicode/rangetable:go_default_library",
    ],
)
